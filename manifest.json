{
    "manifest_version": 2,

    "name": "Twitch Chat Colours",
    "version": "0.0.1",

    "description": "Changes Twitch chat colours.",
    "homepage_url": "http://www.twitch.tv/oshi7",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "default_locale": "en",
    "permissions": [
        "http://www.twitch.tv/oshi7"
    ],
    "content_scripts": [{
        "matches": [
            "http://www.twitch.tv/oshi7"
        ],
        "js": [
            "jquery.js",
            "inject.js"
        ]
    }]
}